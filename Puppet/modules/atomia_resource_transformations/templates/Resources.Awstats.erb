<resourceDescription xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <bindings xdt:Locator="XPath(//bindings[moduleList/module/@name='Atomia.Provisioning.Modules.AWStats.AWStats'])">
    <resourceList>
      <resource name="AWStats" xdt:Transform="Insert">
        <property name="URL">http://<%= @awstats_ip %>:8888/AwstatsAgentService</property>
        <property name="Username"><%= @awstats_agent_user %></property>
        <property name="Password"><%= @awstats_agent_user_agent_password %></property>
        <property name="ReportsSiteIP"><%= @awstats_ip %></property>
      </resource>
    </resourceList>
  </bindings>
</resourceDescription>
