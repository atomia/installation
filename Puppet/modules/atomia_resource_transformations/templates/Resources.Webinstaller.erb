<resourceDescription xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <bindings xdt:Locator="XPath(//bindings[moduleList/module/@name='Atomia.Provisioning.Modules.WebInstaller.WebInstaller'])">
    <resourceList>
      <resource name="Webinstaller" xdt:Transform="Insert">
        <property name="URL">http://<%= @webinstaller_ip %>/webinstaller</property>
        <property name="Username"><%= @webinstaller_username %></property>
        <property name="Password"><%= @webinstaller_password %></property>
        <property name="Timeout">600</property>
	<property name="StoragePath"><%= @atomia_web_content_mount_point %></property>
      </resource>
    </resourceList>
  </bindings>
</resourceDescription>
