<resourceDescription xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <bindings xdt:Locator="XPath(//bindings[moduleList/module/@name='Atomia.Provisioning.Modules.Apache.Apache'])">
    <resourceList>
      <resource name="apache" xdt:Transform="Insert">
        <property name="URL">http://<%= @apache_agent_ip %>:9999/ApacheAgentService?wsdl</property>
        <property name="Username"><%= @apache_agent_user %></property>
        <property name="Password"><%= @apache_agent_password %></property>
        <property name="ClusterIpAddress"><%= @apache_cluster_ip %></property>
      </resource>
    </resourceList>
  </bindings>
</resourceDescription>
